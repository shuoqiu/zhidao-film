.content {
    min-height: 100%;
    min-width: 100%;
    background-color: RGBA(255,255,255,.1);
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
}

.background {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
  z-index: -1000;
  opacity: .2;
}

.img image{
  padding-top: 10px;
}



.info {
  font-size: 24rpx;
  color: #999;
  margin-top: 20rpx;
  width: 80%;
  text-align: center;
}



.summary {
  margin: 10px;
}

.summaryContent {
    color: #000;
    padding: 5px;
    font-size: 13px;
}

.text {
    color: #000;
    font-size: 15px;
    padding-left: 5px;
    padding-bottom: 5px;
}

.castContent {
    display: flex;
    flex-wrap: row wrap;
}

.actorIntroduction{
    width: 25%;
    margin-bottom: 20rpx;
    text-align: center;
}

.actorIntroduction image {
    height: 240rpx;
    width: 160rpx;
    padding: 0px 13rpx 0px 13rpx;
}

.actorName {
    color: #000;
    font-size: 8pt;
    padding-top: 3px;
}

.shareBtn {
  margin: 0 auto; 
  width: 80rpx;
  height: 80rpx; 
  position: fixed; 
  top:280rpx; 
  right:40rpx;   
  font-size: 0; 
  line-height: 0; 
  z-index: 100; 
  text-align: right; 
  border-radius: 40rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  /*背景图片自适应*/
  background-size: 100% 100%;
}


