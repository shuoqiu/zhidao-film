/* pages/search/search.wxss */
page{
    min-width: 100%;
    min-height: 100%;
}
.content{
    background: #3a3a3a;
    min-height: 100%;
    min-width: 100%;
}
.top{
    display: flex;
    flex-direction: row; 
    width: 100%;
    height: 30%;
    /* align-items: center; */
    justify-content: center;  
    padding: 10px 0px;
    background-color: #09BB07;
}
.icon_search {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  width: 10%;
  height: 35px;
  background-color: #fff;
  border-radius: 5px;
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px; 
}

.srk{
    /* border: #fff solid 1px; */
    background-color: #fff;
    border-radius: 5px;
    border-top-left-radius: 0px;
    border-bottom-left-radius: 0px;
    height: 35px;
    line-height: 35px;
    font-size: 15px;
    width: 86%;
    color: #000;
}
